{
  "name": "react-redux-starter",
  "version": "0.0.0",
  "description": "React Redux boilerplate",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel start:dev lint:watch test:watch",
    "start:dev": "node_modules/.bin/babel-node tools/devServer.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/setup.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "lint": "node_modules/.bin/esw webpack.config.* src tools test",
    "lint:watch": "npm run lint -- --watch",
    "remove-dist": "rm -r dist/ && mkdir dist",
    "prebuild": "babel-node tools/buildMessage.js && cp src/index.html dist/index.html",
    "build": "webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "start:dist": "NODE_ENV=production babel-node tools/prodServer.js",
    "build:start": "build && start:prod"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.98labs.com:john.escuyos/react-redux-starter.git"
  },
  "author": "jeescu",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.2.1",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.1.18",
    "babel-preset-stage-1": "6.1.18",
    "chai": "3.5.0",
    "chai-immutable": "1.5.3",
    "chai-jquery": "2.0.0",
    "css-loader": "0.25.0",
    "enzyme": "2.4.1",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.10.2",
    "eslint-plugin-jsx-a11y": "1.5.5",
    "eslint-plugin-react": "5.2.2",
    "eslint-watch": "2.1.13",
    "jquery": "2.2.1",
    "jsdom": "8.1.0",
    "mocha": "2.4.5",
    "npm-run-all": "^3.1.1",
    "react-addons-test-utils": "0.14.7",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-logger": "2.6.1",
    "style-loader": "0.13.1",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0",
    "webpack-hot-middleware": "2.12.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "expect": "1.20.2",
    "immutable": "3.7.6",
    "lodash": "3.10.1",
    "open": "0.0.5",
    "react": "0.14.3",
    "react-dom": "0.14.3",
    "react-redux": "4.0.0",
    "react-router": "2.0.1",
    "redux": "3.0.4",
    "redux-thunk": "2.1.0"
  }
}
